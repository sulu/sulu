<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="sulu_content.export.webspace.class">Sulu\Component\Content\Export\Webspace</parameter>
    </parameters>

    <services>
        <service id="sulu_content.export.webspace" class="%sulu_content.export.webspace.class%">
            <argument type="service" id="templating" />
            <argument type="collection">
                <argument key="1.2.xliff">SuluContentBundle:Export:Webspace/1.2.xliff.twig</argument>
            </argument>
            <!-- <argument type="service" id="sulu_content.structure.loader.xml" /> -->
        </service>
    </services>
</container>
