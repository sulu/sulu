{
    "name": "sulu/resource-bundle",
    "type": "sulu-bundle",
    "description": "The sulu bundle for managing resources",
    "license": "MIT",
    "keywords": ["resource"],
    "version": "dev-develop",
    "authors": [
        {
            "name": "Sulu Community"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "doctrine/orm": "2.4.*",
        "friendsofsymfony/rest-bundle": "1.4.*",
        "friendsofsymfony/http-cache": "1.0.*",
        "stof/doctrine-extensions-bundle": "1.1.*",
        "willdurand/hateoas-bundle": "0.3.*",

        "sulu/sulu": "dev-develop"
    },
    "require-dev": {
        "sensio/framework-extra-bundle":  "~3.0",
        "symfony/symfony": "2.5.*",
        "doctrine/doctrine-bundle": "1.2.*",
        "symfony/monolog-bundle": "2.4.*",
        "phpunit/phpunit": "4.0.*",
        "sulu/test-bundle": "dev-develop",
        "sulu/security-bundle": "dev-develop"
