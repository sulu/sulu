        "doctrine/inflector": "^1.4.1 || ^2.0.1",
        "doctrine/orm": "^2.5.3",
        "doctrine/persistence": "^1.3 || ^2.0",
        "doctrine/phpcr-bundle": "^1.3.5 || ^2.0",
