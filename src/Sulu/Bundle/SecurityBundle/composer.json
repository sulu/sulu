{
    "name": "sulu/security-bundle",
    "type": "sulu-bundle",
    "description": "Security Bundle for the Sulu CMS",
    "license": "MIT",
    "keywords": ["sulu", "security"],
    "version": "0.3.0",
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/massiveart/SuluSecurityBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "friendsofsymfony/rest-bundle": "dev-master",
        "jms/serializer-bundle": "@dev",
        "stof/doctrine-extensions-bundle": "1.1.*@dev",

        "sulu/sulu": "dev-develop",
        "sulu/admin-bundle": "dev-develop",
        "sulu/contact-bundle": "dev-develop"
    },
    "require-dev": {
        "sensio/framework-extra-bundle":  "~3.0",
        "symfony/symfony": "2.4.*",
        "doctrine/doctrine-bundle": "1.2.*",
        "symfony/monolog-bundle": "2.4.*",
        "phpunit/phpunit": "3.7.*",
        "sulu/test-bundle": "dev-develop"
    },
    "autoload": {
        "psr-0": {
            "Sulu\\Bundle\\SecurityBundle": ""
        }
    },
    "target-dir": "Sulu/Bundle/SecurityBundle"
}
