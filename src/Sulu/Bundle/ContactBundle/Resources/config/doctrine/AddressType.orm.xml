<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Sulu\Bundle\ContactBundle\Entity\AddressType" table="co_address_types"
            repository-class="Sulu\Bundle\ContactBundle\Entity\AddressTypeRepository">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="name" type="string" column="name" length="60"/>
        <one-to-many field="addresses" target-entity="Sulu\Bundle\ContactBundle\Entity\Address"
                     mapped-by="addressType"/>
    </entity>
</doctrine-mapping>
