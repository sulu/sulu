{
    "name": "sulu/sulu",
    "type": "library",
    "description": "Symfony bundle that provides basic core functionality",
    "license": "MIT",
    "keywords": ["core", "sulu"],
    "version": "dev-develop",
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/massiveart/SuluContactBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "symfony/framework-bundle": "2.5.*",
        "sensio/distribution-bundle": "2.3.*",
        "friendsofsymfony/rest-bundle": "dev-master",
        "friendsofsymfony/http-cache": "dev-master",
        "jackalope/jackalope-jackrabbit": "1.1.*",
        "doctrine/phpcr-bundle": "1.1.*",
        "doctrine/phpcr-odm": "1.1.*",
        "willdurand/hateoas-bundle": "0.3.*"
    },
    "require-dev": {
        "symfony/symfony": "2.5.*",
        "phpunit/phpunit": "4.0.*",
        "phpspec/prophecy-phpunit": "~1.0",
        "jackalope/jackalope-doctrine-dbal": "1.1.*",
        "matthiasnoback/symfony-dependency-injection-test": "~0.1",
        "matthiasnoback/symfony-config-test": "~0.3",
        "sulu/test-bundle": "dev-feature/node-helper"
    },
    "suggest": {
        "massive/build-bundle": "For initializing the Sulu environment"
    },
    "autoload": {
        "psr-0": {
            "Sulu\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
