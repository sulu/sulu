        "friendsofsymfony/rest-bundle": "^2.8 || ^3.0",
        "gedmo/doctrine-extensions": "^2.4 || ^3.0",
        "guzzlehttp/promises": "^1.0",
        "handcraftedinthealps/goodby-csv": "^1.4",
